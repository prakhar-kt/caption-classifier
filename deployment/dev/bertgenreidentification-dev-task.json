{
    "family": "bertgenreidentification-service-dev-task",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "executionRoleArn": "arn:aws:iam::728291931126:role/ecsTaskExecutionRole",
    "cpu": "2048",
    "memory": "4096",
    "containerDefinitions": [
      {
        "name": "bertgenreidentification-service-dev-task",
        "image": "728291931126.dkr.ecr.ap-south-1.amazonaws.com/bertgenreidentification-service:%APP_IMAGE_TAG%",
        "portMappings": [
          {
            "hostPort": 3004,
            "protocol": "tcp",
            "containerPort": 3004
          }
        ],
        "cpu": 0,
        "essential": true,
        "secrets": [
          {         
            "valueFrom": "arn:aws:ssm:ap-south-1:728291931126:parameter/dev/bertgenreidentification-service/APP_PORT",
            "name": "APP_PORT"
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-create-group": "true",
            "awslogs-group": "/ecs/bertgenreidentification-service-dev-task",
            "awslogs-region": "ap-south-1",
            "awslogs-stream-prefix": "ecs"
          }
        }
      },
      {
        "name": "nginx",
        "image": "728291931126.dkr.ecr.ap-south-1.amazonaws.com/bertgenreidentification-service:%NGINX_IMAGE_TAG%",
        "portMappings": [
          {
            "hostPort": 80,
            "protocol": "tcp",
            "containerPort": 80
          }
        ],
        "cpu": 0,
        "essential": true,
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-create-group": "true",
            "awslogs-group": "/ecs/bertgenreidentification-service-dev-task",
            "awslogs-region": "ap-south-1",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ]
  }
  